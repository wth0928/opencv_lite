cc_library_shared {
	name: "zlib",
	rtti: true,
	vendor_available: true,
	vndk: {
		enabled: true,
	},
	srcs: [
		"adler32.c",
		"compress.c",
		"crc32.c",
		"deflate.c",
		"gzclose.c",
		"gzlib.c",
		"gzread.c",
		"gzwrite.c",
		"inflate.c",
		"infback.c",
		"inftrees.c",
		"inffast.c",
		"trees.c",
		"uncompr.c",
		"zutil.c",	
	],
	include_dirs: [
		"external/opencv-4.3.0/3rdparty/zlib",
	],
	cflags: [
		"-DHAVE_UNISTD_H",
		"-Wall",
		"-Werror",
		"-Wextra",
	],
	shared_libs: [
		"libcutils",
		"libutils",
		"liblog",
	],
	stl: "libc++_static",
}
